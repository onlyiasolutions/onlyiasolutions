{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    { "source": "/(.+)\\.html", "destination": "/$1", "permanent": true },
    { "source": "/index.html", "destination": "/", "permanent": true }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
